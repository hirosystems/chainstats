  "main": "index.ts",
  "prettier": "@stacks/prettier-config",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "concurrently --kill-others \"pnpm run schedule\" \"pnpm run serve\"",
    "serve": "node ./build/server.js",
    "serve:dev": "cross-env NODE_ENV=development nodemon -e ts -x 'ts-node-transpile-only src/server/index.ts'",
    "schedule": "node ./build/jobs.js",
    "schedule:dev": "cross-env NODE_ENV=development nodemon -e ts -x 'ts-node-transpile-only src/jobs.ts'",
    "generateJson": "cross-env NODE_ENV=development nodemon -e ts -x 'ts-node-transpile-only src/generateJson.ts'",
    "prettier:fix": "prettier --write \"src/**/*.{ts,,js,,css,scss}\"",
    "migrate:dev": "npx prisma migrate dev --name init",
    "migrate": "npx prisma migrate deploy",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@bull-board/fastify": "^5.2.0",
